pipeline {
  agent {
    label 'test'
  }
  stages {
    stage('test') {
      steps {
        echo ' test stage ran'
        echo 'new line'
        echo ' success '
        dir(path: 'repo') {
          checkout scm
          command 'git fetch --all'
          command "${toolbelt}/sfdx sfpowerkit:project:diff -d deltaTest2 -x -r master -t test-plugin"
          script {
            echo 'Script in repo'
            command "${toolbelt}/sfdx sfpowerkit:project:diff -d deltaTest2 -x -r master -t test-plugin"
          }

        }

        script {
          SFDX = command "${toolbelt}/sfdx force:org:list"
          echo "command: ${SFDX}"
          SFDX_2 = command "${toolbelt}/sfdx force:org:list"
          echo "2nd: ${SFDX_2}"
        }

        sh '"${toolbelt}/sfdx sfpowerkit:project:diff -d deltaTest2 -x -r master -t test-plugin"'
      }
    }

    stage('deployment') {
      steps {
        echo 'Deployment Stage'
      }
    }

  }
  environment {
    toolbelt = 'sfdx_toolbelt'
  }
}